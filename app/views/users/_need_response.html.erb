<table class='user-table table'>
  <tr>
    <th>Name</th>
    <th>Applied on</th>
    <th>Quick review</th>
    <th>Up votes</th>
    <th>Down votes</th>
  </tr>
  <% users.each do |user| %>
    <tr>
      <td>
        <%= link_to user.name, user_path(user) %>
      </td>
      <td>
        <%= user.created_at.strftime('%B %d, %Y %I:%M %P') %>
      </td>
      <td>
        <%= user.important_attributes.join(', ') %>
      </td>
      <td>
        <%= user.count_up_votes %>
      </td>
      <td>
        <%= user.count_down_votes %>
      </td>
    </tr>
  <% end %>
</table>