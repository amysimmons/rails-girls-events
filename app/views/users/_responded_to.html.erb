<table class='user-table table'>
  <tr>
    <th class="icon-cell">Our offer</th>
    <th>Name</th>
    <th class="icon-cell">Their response</th>
    <th>Quick review</th>
  </tr>
  <% users.each do |user| %>
    <tr>
      <td class="icon-cell">
        <i class="fa <%= user.admin_status_icon %>"></i>
      </td>
      <td>
        <%= link_to user.name, user_path(user) %>
      </td>
      <td class="icon-cell">
        <i class="fa <%= user.user_status_icon %>"></i>
      </td>
      <td>
        <%= user.important_attributes.join(', ') %>
      </td>
    </tr>
  <% end %>
</table>