<div class="container_12 events-container container">
  <table class="table">
    <tr>
      <th colspan="4"><h1>Upcoming events</h1></th>
    </tr>
    <tr>
      <th>Title</th>
      <th>Applicants</th>
      <th>Data</th>
    </tr>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= link_to 'Applicants', event_path(event) %></td>
        <td><%=link_to 'Data', event_data_path(event) %></td>
      </tr>
    <% end %>
  </table>
</div>

